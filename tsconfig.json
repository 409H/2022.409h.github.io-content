{
    "compilerOptions": {
      "noEmit": true,
      "target": "esnext",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": false,
      "noEmitOnError": true,
      "esModuleInterop": true,
      "declaration": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "include": ["deploy/deploy.ts"]
}